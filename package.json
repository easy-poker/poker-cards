{
  "name": "@pairjacks/poker-cards",
  "version": "0.1.1",
  "private": false,
  "description": "Handles cards, decks and hands for poker",
  "repository": {
    "type": "git",
    "url": "https://github.com/pairjacks/poker-cards.git"
  },
  "license": "MIT",
  "author": "Kav Singh <flakisms@gmail.com>",
  "contributors": [],
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "./src/index.ts",
  "scripts": {
    "test": "jest",
    "test:mutation": "stryker run",
    "lint": "tsc && eslint . --ext .ts,.js",
    "prebuild": "rm -rf lib && rm -rf dist",
    "build:js": "NODE_ENV=production rollup --config",
    "build:declarations": "tsc --project ./tsconfig.declarations.json",
    "build:extract_api": "api-extractor run --local",
    "build:bundle_package_json": "ts-node -P tsconfig.tsnode.json scripts/bundle-package-json.ts",
    "build:copy_readme": "cp README.md ./dist",
    "build": "npm run build:js && npm run build:declarations && npm run build:extract_api && npm run build:bundle_package_json && npm run build:copy_readme",
    "postbuild": "rm -rf lib"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.9.0",
    "@microsoft/api-extractor": "7.8.4",
    "@rollup/plugin-commonjs": "12.0.0",
    "@rollup/plugin-node-resolve": "8.0.0",
    "@rollup/plugin-typescript": "4.1.2",
    "@stryker-mutator/core": "3.2.4",
    "@stryker-mutator/jest-runner": "3.2.4",
    "@stryker-mutator/typescript": "3.2.4",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.5",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "babel-jest": "26.0.1",
    "core-js": "3.6.5",
    "eslint": "7.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-tsdoc": "0.2.5",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "rollup": "2.10.7",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-cleanup": "3.1.1",
    "ts-node": "8.10.1",
    "tslib": "2.0.0",
    "typescript": "3.9.3"
  }
}
