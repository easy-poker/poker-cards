{
  "name": "poker-cards",
  "version": "0.0.12",
  "description": "Handles cards, decks and hands for poker",
  "repository": {
    "type": "git",
    "url": "https://github.com/pairjacks/poker-cards.git"
  },
  "license": "MIT",
  "author": "Kav Singh <flakisms@gmail.com>",
  "contributors": [],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@pairjacks"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "./src/index.ts",
  "scripts": {
    "test": "jest",
    "test:mutation": "stryker run",
    "lint": "tsc && eslint . --ext .ts,.js",
    "prebuild": "rm -rf lib && rm -rf dist",
    "build:js": "NODE_ENV=production rollup --config",
    "build:declarations": "tsc --project ./tsconfig.declarations.json",
    "build:extract_api": "api-extractor run --local",
    "build:bundle_package_json": "ts-node -P tsconfig.tsnode.json scripts/bundle-package-json.ts",
    "build:copy_readme": "cp README.md ./dist",
    "build": "npm run build:js && npm run build:declarations && npm run build:extract_api && npm run build:bundle_package_json && npm run build:copy_readme",
    "postbuild": "rm -rf lib",
    "deploy": "cd dist && npm publish"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-typescript": "7.9.0",
    "@microsoft/api-extractor": "7.7.13",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@rollup/plugin-typescript": "4.0.0",
    "@stryker-mutator/core": "3.1.0",
    "@stryker-mutator/jest-runner": "3.1.0",
    "@stryker-mutator/typescript": "3.1.0",
    "@types/jest": "25.2.1",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "25.3.0",
    "core-js": "3.6.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-tsdoc": "0.2.4",
    "jest": "25.3.0",
    "prettier": "2.0.4",
    "rollup": "2.6.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-cleanup": "3.1.1",
    "ts-node": "8.8.2",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  }
}
